{
  "name": "adonis-url-signer",
  "version": "1.0.0",
  "description": "URL signing provider for Adonis.js",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "lint-staged": "npm run test:win && lint-staged",
    "test:local": "NO_ANSI=false FORCE_COLOR=true nyc node japaFile",
    "test:win": "set NO_ANSI=false && set FORCE_COLOR=true && nyc node japaFile",
    "test": "npm run test:local",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/sink": "^1.0.17",
    "coveralls": "^3.0.3",
    "husky": "^1.3.1",
    "japa": "^2.0.7",
    "lint-staged": "^8.1.5",
    "nyc": "^13.3.0",
    "standard": "^12.0.1"
  },
  "nyc": {
    "include": [
      "src",
      "providers"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "adonis-framework",
    "url-sign",
    "node"
  ],
  "standard": {
    "globals": [
      "use",
      "make"
    ]
  },
  "author": "Kashyap Merai <kashyapk62@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamerk22/adonis-url-signer"
  },
  "bugs": {
    "url": "https://github.com/kamerk22/adonis-url-signer/issues"
  },
  "homepage": "https://github.com/kamerk22/adonis-url-signer#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "dependencies": {}
}
